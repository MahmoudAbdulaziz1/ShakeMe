<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#FFFFFF">
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/select">

    <Spinner
        android:layout_width="fill_parent"
        android:layout_height="90dp"
        android:id="@+id/servicesf"
        android:spinnerMode="dropdown"
        android:layout_marginTop="4dp"
        android:layout_toLeftOf="@+id/fline"
        android:layout_toStartOf="@+id/fline"
        android:background="@drawable/getspinner"/>

    <Button
        android:layout_width="240dp"
        android:layout_height="70dp"
        android:text="@string/select_app_button"
        android:textSize="40sp"
        android:id="@+id/selectAppf"
        android:layout_below="@+id/servicesf"
        android:background="@drawable/get"
        android:textColor="#000000"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageViewf"
        android:layout_alignRight="@+id/fline"
        android:layout_alignEnd="@+id/fline"
        android:layout_toRightOf="@+id/selectAppf"
        android:layout_toEndOf="@+id/selectAppf"
        android:layout_alignTop="@+id/selectAppf"
        android:layout_alignBottom="@+id/selectAppf" />

    <View
        android:layout_width="12dp"
        android:layout_height="fill_parent"
        android:background="#66FFFF"
        android:id="@+id/fline"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textviewcf"
        android:text="@string/item31"
        android:textSize="35dp"
        android:textColor="#FFCC00"
        android:layout_below="@+id/textViewofcheckfor"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />



    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="400dp"
        android:id="@+id/imageView2"
        android:src="@drawable/forshake"
        android:layout_toRightOf="@+id/imageViewf"
        android:layout_toEndOf="@+id/imageViewf"
        android:layout_alignParentBottom="true" />

    <View
        android:layout_width="fill_parent"
        android:layout_height="8dp"
        android:background="#66FFFF"
        android:id="@+id/fline1"
        android:layout_below="@+id/selectAppf"
        android:layout_alignRight="@+id/fline"
        android:layout_alignEnd="@+id/fline"
        android:layout_marginTop="20dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/sensor_senstivity"
        android:textStyle="bold"
        android:id="@+id/titles_sensorf"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="40dp"
        android:textColor="#000000"
        android:layout_below="@+id/fline1"
        android:layout_alignRight="@+id/fline1"
        android:layout_alignEnd="@+id/fline1"
        android:layout_marginTop="8dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/sensor_sensitvityd"
        android:id="@+id/dess_sensorf"
        android:textSize="35dp"
        android:textColor="#FFCC00"
        android:layout_below="@+id/titles_sensorf"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        />
    <View
        android:layout_width="fill_parent"
        android:layout_height="8dp"
        android:background="#66FFFF"
        android:id="@+id/fline2"
        android:layout_below="@+id/dess_sensorf"
        android:layout_alignRight="@+id/fline"
        android:layout_alignEnd="@+id/fline"
        android:layout_marginTop="20dp"/>
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/checkBoxscf"
        android:checked="false"
        android:button="@xml/custom_check_box"
        android:layout_below="@+id/fline2"
        android:layout_alignRight="@+id/fline2"
        android:layout_alignEnd="@+id/fline2"
        android:layout_marginTop="4dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/item4"
        android:id="@+id/textViewofcheckf"
        android:textSize="40dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/fline2"
        android:layout_marginTop="8dp"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textviewcfor"
        android:text="@string/item41"
        android:textSize="35dp"
        android:textColor="#FFCC00"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/fline"
        android:layout_alignEnd="@+id/fline"
        android:layout_below="@+id/textViewofcheckf" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/checkBoxvf"
        android:checked="false"
        android:button="@xml/custom_check_box"
        android:layout_toLeftOf="@+id/fline"
        android:layout_below="@+id/textviewcfor"
        android:layout_alignLeft="@+id/checkBoxscf"
        android:layout_alignStart="@+id/checkBoxscf" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:text="@string/item3"
        android:id="@+id/textViewofcheckfor"
        android:textSize="40dp"
        android:textColor="#000000"
        android:textStyle="bold"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textviewcfor" />
    <View
        android:layout_width="fill_parent"
        android:layout_height="8dp"
        android:background="#66FFFF"
        android:id="@+id/fline4"
        android:layout_marginTop="112dp"
        android:layout_below="@+id/checkBoxscf"
        android:layout_alignRight="@+id/fline"
        android:layout_alignEnd="@+id/fline" />




</RelativeLayout>
</ScrollView>